# Instagram credentials
INSTAGRAM_USERNAME=your_username
INSTAGRAM_PASSWORD=your_password
INSTAGRAM_2FA_ENABLED=True

# Browser settings
HEADLESS_MODE=False
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/90.0.4430.212 Safari/537.36
USER_AGENT_ROTATION=True
BROWSER_LANGUAGE=en-US,en;q=0.9

# Proxy settings
USE_PROXY=False
PROXY_LIST_PATH=proxies.txt
PROXY_VALIDATION=True
PROXY_ROTATION_INTERVAL=30

# Anti-detection settings
SIMULATE_HUMAN_BEHAVIOR=True
RANDOM_MOUSE_MOVEMENTS=True
REALISTIC_TYPING=True
RANDOM_SCROLLING=True

# Scraping settings
REQUEST_TIMEOUT=30
MAX_RETRIES=3
DELAY_BETWEEN_REQUESTS_MIN=2.0
DELAY_BETWEEN_REQUESTS_MAX=5.0
MAX_REQUESTS_PER_SESSION=100
SESSION_DURATION_MAX=3600

# Error handling
EXPONENTIAL_BACKOFF=True
SAVE_ERROR_SCREENSHOTS=True
ERROR_SCREENSHOT_DIR=logs/screenshots

# Data storage
DATA_DIR=data
SESSION_DIR=data/sessions 